{
  "name": "k-ui-dashboard-alpine",
  "version": "0.0.1",
  "description": "Responsive dashboard template built with tailwindcss & alpinejs",
  "scripts": {
    "serve": "cross-env NODE_ENV=development concurrently \"postcss src/tailwind.css -o public/assets/css/tailwind.css --watch\"  \"live-server ./public\"",
    "development": "cross-env NODE_ENV=development postcss src/tailwind.css -o public/assets/css/tailwind.css",
    "production": "cross-env NODE_ENV=production postcss src/tailwind.css -o public/assets/css/tailwind.css",
    "start": "npm run serve",
    "playground": "live-server ./playground",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "deploy": "gh-pages -d public"
  },
  "keywords": [
    "dashboard",
    "tailwindcss",
    "layout",
    "alpinejs",
    "admin",
    "template"
  ],
  "author": {
    "name": "Ahmed Kamel",
    "email": "ak3849909@gmail.com",
    "url": "https://github.com/kamona-wd"
  },
  "license": "MIT",
  "dependencies": {
    "gh-pages": "^3.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "live-server": "^1.2.1",
    "postcss": "^8.2.13",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.1.2",
    "vuepress": "^1.8.2"
  }
}
